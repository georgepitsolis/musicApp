<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".SoulActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/soul">
        </activity>

        <activity android:name=".CountryActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/country">
        </activity>

        <activity android:name=".JazzActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/jazz">
        </activity>

        <activity android:name=".PopActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/pop">
        </activity>

        <activity android:name=".SongActivity"
            android:label="@string/play_mode">
        </activity>
    </application>

</manifest>